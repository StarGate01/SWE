    "configurations": [
        {
            "name": "GDB Linux Native",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/build/SWE_gnu_debug_none_fwave",
            "args": [
                "-x", "50", "-y", "50", 
                "-a", "../input/artificial/artificialtsunami_bathymetry_1000.nc",
